{
    "engines": 
    {
      "node": "16.13.0"
    },
  "functions": {
    "api/**/*.{js,ts}": {
      "runtime": "@vercel/node"
    }
  },
  "builds": [
    {
      "src": "vercel-build.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": "build/**"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/build/$1"
    }
  ]
}
